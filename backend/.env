# CloudWeave Go Backend Environment Configuration

# Application
NODE_ENV=development
PORT=3001

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=cloud_platform_db
DB_USER=coryjanowski
DB_PASSWORD=
DB_SSL_MODE=disable

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-that-should-be-changed-in-production
JWT_EXPIRES_IN=24h
JWT_REFRESH_EXPIRES_IN=7d

# Security
BCRYPT_ROUNDS=12
CORS_ORIGIN=*# SSO Configuration for Testing
SSO_OAUTH_ENABLED=true

# Google OAuth (for testing - you'll need real credentials)
SSO_GOOGLE_ENABLED=true
SSO_GOOGLE_CLIENT_ID=test-google-client-id
SSO_GOOGLE_CLIENT_SECRET=test-google-client-secret
SSO_GOOGLE_REDIRECT_URL=http://localhost:5173/auth/callback/google

# Microsoft OAuth (for testing)
SSO_MICROSOFT_ENABLED=true
SSO_MICROSOFT_CLIENT_ID=test-microsoft-client-id
SSO_MICROSOFT_CLIENT_SECRET=test-microsoft-client-secret
SSO_MICROSOFT_REDIRECT_URL=http://localhost:5173/auth/callback/microsoft

# GitHub OAuth (for testing)
SSO_GITHUB_ENABLED=true
SSO_GITHUB_CLIENT_ID=test-github-client-id
SSO_GITHUB_CLIENT_SECRET=test-github-client-secret
SSO_GITHUB_REDIRECT_URL=http://localhost:5173/auth/callback/github